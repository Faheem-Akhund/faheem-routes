                \n                 }\n\n          })\n          .then((response) => {\n            //  console.log(response);\n             if(response.status===200)\n            {\n                 this.Students=response.data\n\n            }\n              return response;\n            })\n            .catch((error) => {\n             \n             console.log(error.response.status);\n             \n            \n            }) \n          ;\n    \n\n\n       \n      \n          \n          \n        \n   \n          \n}\n}\n</script>\n\n<style>\ntable, th, td {\n  border: 1px solid black;\n  border-collapse: collapse;\n}\n</style>"],"sourceRoot":""}]}